{
  "name": "village-on",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint --fix",
    "build:script": "esbuild src/shared/api/open-api/script.ts --bundle --platform=node --format=cjs --tsconfig=tsconfig.script.json --outfile=script/bundle.cjs",
    "start:script": "node script/bundle.cjs",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:typegen": "dotenv -e .env -- bash -c 'supabase gen types typescript --project-id $VITE_SUPABASE_PROJECT_ID > ./src/shared/database/database.types.ts'",
    "build:android": "pnpm run build && exec cap copy android && exec cap sync android",
    "run:android": "exec cap open android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.0",
    "@capacitor/app": "7.0.1",
    "@capacitor/core": "7.4.0",
    "@capacitor/haptics": "7.0.1",
    "@capacitor/ios": "^7.4.0",
    "@capacitor/keyboard": "7.0.1",
    "@capacitor/status-bar": "7.0.1",
    "@ionic/react": "^8.5.0",
    "@ionic/react-router": "^8.5.0",
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-query": "^5.81.2",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "drizzle-orm": "^0.44.2",
    "ionicons": "^7.4.0",
    "postgres": "^3.4.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-intersection-observer": "^9.16.0",
    "react-router": "^5.3.4",
    "react-router-dom": "^5.3.4",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "@capacitor/cli": "7.4.0",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@testing-library/dom": ">=7.21.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.1",
    "cypress": "^13.5.0",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.20.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^22.1.0",
    "supabase": "^2.26.9",
    "terser": "^5.4.0",
    "ts-node": "^10.9.2",
    "type-fest": "^4.41.0",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.24.0",
    "vite": "~5.2.0",
    "vitest": "^0.34.6"
  },
  "description": "An VillageOn project",
  "packageManager": "pnpm@9.15.9+sha512.68046141893c66fad01c079231128e9afb89ef87e2691d69e4d40eee228988295fd4682181bae55b58418c3a253bde65a505ec7c5f9403ece5cc3cd37dcf2531"
